{
  "expo": {
    "name": "CricsHub",
    "slug": "CricsHub",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "cricshub",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.cricshub.team",
      "buildNumber": "1"
    },
    "android": {
      "package": "dev.cricshub.team",
      "versionCode": 27,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/iconLogo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "BASE_URL": "https://app.cricshub.com/api/v1",
      "router": {},
      "eas": {
        "projectId": "4c4b1af0-2ee1-49a4-a64a-f084909f09fc"
      }
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "owner": "cricshub"
  }
}
